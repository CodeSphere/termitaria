<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<!--
  ===========================================================================
  ====================== ORGANISATION MANAGEMENT ============================
  ===========================================================================
  =========================== LOGO ==========================================
 -->
<hibernate-mapping auto-import="true" default-lazy="false">

    <class name="ro.cs.om.entity.Logo" entity-name="Logo" table="logo">
        <id name="logoId" column="LOGOID">
            <generator class="native" />
        </id>
        <property name="picture" column="PICTURE" />
        <property name="dateCreated" column="DATE_CREATED" />
        <property name="dateModified" column="DATE_MODIFIED" />
        <property name="organisationId" column="ORGANISATIONID" />
        <property name="extension" column="EXTENSION" />
    </class>

</hibernate-mapping>
